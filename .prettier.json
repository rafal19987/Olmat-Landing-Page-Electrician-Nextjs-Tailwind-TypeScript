{
  "trailingComma": "es5",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^@(.*)$",
    "^@/(.*)$",
    "^@/types/(.*)$",
    "^@/app/(.*)$",
    "^@/lib/(.*)$",
    "^@/assets/(.*)$",
    "^@/components/(.*)$",
    "^[./]"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
